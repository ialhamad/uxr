extends /specs/products/aps/page-structure/structure/templates/structure/layout-tmpl.jade

block content
    include /specs/page-structure/helpers/print-json-mixin.jade
    include /specs/page-structure/helpers/print-js-code-mixin.jade

    section.source_section
        h2 Usage
        h3 Data
        p The following data structure must exist to be able to use Supplemental Content Type template.

        +printJson({
            "data": {
                "value": "dataset",
                "type": "list",
                "listId": "list1"

            }
        }
        )



    | #{''}

    section.source_section
        h2 Supplemental Content Type list
        .source_example
            | !{pb.render('supplemental-content-type', 'data.json', {"shared": {"model": {"config": {"wrapper": "false"}}}})}

    | #{''}

    section.source_section
        h2 Supplemental Content Type Get selected item
        .source_example
            | !{pb.render('supplemental-content-type', 'data2.json', {"shared": {"model": {"config": {"wrapper": "false"}}}})}

    | #{''}